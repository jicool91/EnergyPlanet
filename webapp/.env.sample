# Webapp Environment Configuration Sample
# Copy this file to .env and fill in your values

# API URL
VITE_API_URL=http://localhost:3000/api/v1

# Environment
VITE_ENV=development

# CDN URL (for assets)
VITE_CDN_URL=https://cdn.energyplanet.game

# Telegram Mini App migration toggles
# Set to "true" to enable the new TMA-based implementations.
# Leave unset or "false" to keep legacy behaviour.
VITE_TMA_MIGRATION_ENABLED=false
VITE_TMA_MIGRATION_SAFE_AREA=false
VITE_TMA_MIGRATION_THEME=false
VITE_TMA_MIGRATION_BACK_BUTTON=false
